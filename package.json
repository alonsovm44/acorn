{
  "name": "acorn",
  "displayName": "Acorn",
  "description": "A new language",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:acorn"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "acorn",
        "aliases": ["Acorn", "acorn"],
        "extensions": [".acn"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "acorn",
        "scopeName": "source.acorn",
        "path": "./syntaxes/acorn.tmLanguage.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "typescript": "^4.7.2",
    "@types/node": "^16.11.7",
    "@types/vscode": "^1.70.0",
    "tslint": "^6.1.3",
    "vscode-test": "^1.6.2"
  }
}
